<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aop="http://www.springframework.org/schema/aop"
    xmlns:tx="http://www.springframework.org/schema/tx"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
        http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">

    <!-- 必ず記載して下さい。AI-JBの設定です。JUnitからDB接続するためにimportします -->
    <import resource="classpath:applicationContext-aijb-job.xml" />

    <!-- FacadeService実装 -->
    <!-- キュー登録部品に関するサービスクラス DI用の設定です -->
    <bean id="requestjobFacadeService"
        class="com.globaldenso.ai.aijb.library.business.service.RequestjobFacadeServiceImpl">
        <property name="queueService" ref="queueService" />
        <property name="jobManageService" ref="jobManageService" />
    </bean>
    
    <!-- 常駐リクエスト登録部品に関するサービスクラス DI用の設定です -->
    <bean id="residentRequestjobFacadeService"
        class="com.globaldenso.ai.aijb.library.business.service.ResidentRequestjobFacadeServiceImpl">
        <property name="aijbResidentReqManageService" ref="aijbResidentReqManageService" />
        <property name="residentRequestService" ref="residentRequestService" />
        <property name="jobManageService" ref="jobManageService" />
        <property name="vwAijbReqManageUnionAllService" ref="vwAijbReqManageUnionAllService" />
    </bean>

    <!-- 常駐リクエスト管理に関するサービスクラス DI用の設定です -->
    <bean id="residentRequestService"
        class="com.globaldenso.ai.aijb.job.residentrequest.business.service.ResidentRequestServiceImpl">
        <property name="residentRequestDao" ref="residentRequestDao" />
    </bean>

    <!-- 常駐リクエスト管理DAO -->
    <bean id="residentRequestDao"
        class="com.globaldenso.ai.aijb.job.residentrequest.integration.dao.ResidentRequestDaoImpl">
        <property name="sqlMapClient">
            <ref bean="sqlMapClient" />
        </property>
    </bean>

    <!-- Utility実装 -->


</beans>