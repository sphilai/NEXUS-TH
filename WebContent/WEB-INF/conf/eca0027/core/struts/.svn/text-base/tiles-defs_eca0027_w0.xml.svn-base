<!DOCTYPE tiles-definitions PUBLIC
            "-//Apache Software Foundation//DTD Tiles Configuration 1.3//EN"
            "http://struts.apache.org/dtds/tiles-config_1_3.dtd">
<tiles-definitions>

    <!-- 個別画面 -->
    <!-- ====================================================================-->

    <!--
        この個別画面の定義名はaction-mappingsのforwardタグと紐付き、
        header.jspにて画面IDとして共通的に埋め込まれます。
        このため、この個別画面の定義名には、画面IDそのものを使用してください。
    -->

    <definition name="W0002" extends=".tiles.layout.main">
        <put name="message_area_height" value="50"/>
        <put name="screenId" value="W0002"/>
        <put name="topFlg" value="true" />
        <put name="css_prefix" value="css/eca0027/core/W0002"/>
        <put name="js_filepath" value="js/eca0027/core/W0002.js"/>
        <put name="body" value="/WEB-INF/jsp/eca0027/core/W0002.jsp" />
    </definition>
</tiles-definitions>