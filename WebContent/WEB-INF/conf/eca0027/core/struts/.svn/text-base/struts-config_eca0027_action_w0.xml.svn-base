<!DOCTYPE struts-config PUBLIC
            "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
            "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>

    <!-- Application Action Mapping Definitions ============================ -->
    <action-mappings>
        <!-- W0002: Menu -->
        <action path="/W0002Init"
            type="com.globaldenso.eca0027.core.presentation.W0002InitAction"
            name="W0002Form"
            scope="request"
            validate="false">
            <forward name="success" path="W0002" />
        </action>
        <!-- W0002: Setup -->
        <action path="/W0002Setup"
            type="com.globaldenso.eca0027.core.presentation.W0002SetupAction"
            name="W0002Form"
            scope="request"
            validate="false">
            <forward name="success" path="/W0002Init.do" />
        </action>
        <!-- リソースクリア(最後に必ず消すこと) -->
        <action path="/W0002ClearResource"
            type="com.globaldenso.eca0027.core.presentation.W0002ClearResourceAction"
            name="W0002Form"
            scope="request"
            validate="false">
            <forward name="success" path="/W0002Init.do" />
        </action>
        
    </action-mappings>

</struts-config>