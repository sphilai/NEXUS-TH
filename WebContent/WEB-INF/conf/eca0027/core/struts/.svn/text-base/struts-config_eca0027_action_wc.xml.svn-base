<!DOCTYPE struts-config PUBLIC
            "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
            "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>

    <!-- Application Action Mapping Definitions ============================ -->
    <action-mappings>

        <!-- =============================================================== -->
        <!-- WB001: Empty RT materials画面                                   -->
        <!-- =============================================================== -->

        <!-- WC001: Empty RT materials画面 Cancelアクション -->
        <action path="/WC001Cancel"
            type="com.globaldenso.eca0027.core.presentation.WC001CancelAction"
            name="WC001Form"
            scope="request"
            validate="false">
            <forward name="success" path="WC001" />
            <forward name="error" path="WC001" />
        </action>

        <!-- WC001: Empty RT materials画面 Clearアクション -->
        <action path="/WC001Clear"
            type="com.globaldenso.eca0027.core.presentation.WC001ClearAction"
            name="WC001Form"
            scope="request"
            validate="false">
            <forward name="success" path="WC001" />
            <forward name="error" path="WC001" />
        </action>

        <!-- WC001: Empty RT materials画面 Displayアクション -->
        <action path="/WC001Display"
            type="com.globaldenso.eca0027.core.presentation.WC001DisplayAction"
            name="WC001Form"
            scope="request"
            validate="false">
            <forward name="success" path="WC001" />
            <forward name="error" path="WC001" />
        </action>

        <!-- WC001: Empty RT materials画面 Initアクション -->
        <action path="/WC001Init"
            type="com.globaldenso.eca0027.core.presentation.WC001InitAction"
            name="WC001Form"
            scope="request"
            validate="false">
            <forward name="success" path="WC001" />
            <forward name="error" path="WC001" />
        </action>

        <!-- WC001: Empty RT materials画面 PrintCmlアクション -->
        <action path="/WC001PrintCml"
            type="com.globaldenso.eca0027.core.presentation.WC001PrintCmlAction"
            name="WC001Form"
            scope="request"
            validate="false">
            <forward name="success" path="WC001" />
            <forward name="error" path="WC001" />
        </action>

        <!-- WC001: Empty RT materials画面 Registerアクション -->
        <action path="/WC001Register"
            type="com.globaldenso.eca0027.core.presentation.WC001RegisterAction"
            name="WC001Form"
            scope="request"
            validate="false">
            <forward name="success" path="WC001" />
            <forward name="error" path="WC001" />
        </action>

    </action-mappings>
</struts-config>