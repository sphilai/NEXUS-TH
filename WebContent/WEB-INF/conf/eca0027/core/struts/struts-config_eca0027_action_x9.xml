<!DOCTYPE struts-config PUBLIC
            "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
            "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>

    <!-- Application Action Mapping Definitions ============================ -->
    <action-mappings>

        <!-- =============================================================== -->
        <!-- X9001: 子画面用テスト親画面                                                                                   -->
        <!-- =============================================================== -->
        
        <action path="/X9001Init"
            type="com.globaldenso.eca0027.core.presentation.X9001InitAction"
            name="X9001"
            scope="request"
            validate="false">
            <forward name="success" path="X9001" />
            <forward name="error" path="X9001" />
        </action>
        
    </action-mappings>
</struts-config>