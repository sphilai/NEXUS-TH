            "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>

    <!-- Application Action Mapping Definitions ============================ -->
    <action-mappings>

        <!-- =============================================================== -->
        <!-- WB001: Order Entry Header画面                                 -->
        <!-- =============================================================== -->

        <!-- WB001: Order Entry Header画面 Initアクション -->
        <action path="/WB001Init"
            type="com.globaldenso.eca0027.core.presentation.WB001InitAction"
            name="WB001Form"
            scope="request"
            validate="false">
            <forward name="success" path="WB001" />
            <forward name="error" path="WB001" />
        </action>

        <!-- WB001: Order Entry Header画面 MoveToRegisterアクション -->
        <action path="/WB001MoveToRegister"
            type="com.globaldenso.eca0027.core.presentation.WB001MoveToRegisterAction"
            name="WB001Form"
            scope="request"
            validate="false">
            <forward name="success" path="/WB002Init.do" />
