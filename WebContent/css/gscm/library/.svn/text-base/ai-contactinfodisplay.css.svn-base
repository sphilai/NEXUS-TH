/* ****** 問合せ先表示エリア ****** */
#contactInfoDisplayArea {
    background: #ffffff;
    border:1px dotted #D3C27C;
    padding:5px 3px;
    margin:5px 0px;
    border-style:solid;
    border-width:1px;
    border-color:#330099;;
}

h1.contactInfoDisplay {
    font-size: 14px;
    color: #330099;
    background: #ffffff;
    /* background: #CCCCCC; ※セーフカラーはこちら*/
    padding: 6px 9px;
    letter-spacing: 0.2em;
    border-top: 4px double #333399;
    border-bottom: 4px double #333399;
    border-right: 1px solid #333399;
    border-left: 1px solid #333399;
}