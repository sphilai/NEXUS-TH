package com.globaldenso.gscm.framework.presentation;

import com.globaldenso.gscm.framework.util.PageInfoData;

/**
 * ページング情報保持クラス。
 */
public class PageInfo extends PageInfoData {

    /**
     * フィールドの役割を記述してください。
     */
    private static final long serialVersionUID = 1L;
    
    /**
     * 検索用パス
     */
    private String searchPath = null;
    
    /**
     * 検索用フォーム
     */
    private String formName = null;

    /**
     * デフォルトコンストラクタ。
     */
    public PageInfo() {
        super();
    }

    /**
     * searchPath のゲッターメソッドです。
     *
     * @return the searchPath
     */
    public String getSearchPath() {
        return searchPath;
    }

    /**
     * searchPath のセッターメソッドです。
     *
     * @param searchPath searchPath に設定する
     */
    public void setSearchPath(String searchPath) {
        this.searchPath = searchPath;
    }

    /**
     * formName のゲッターメソッドです。
     *
     * @return the formName
     */
    public String getFormName() {
        return formName;
    }

    /**
     * formName のセッターメソッドです。
     *
     * @param formName formName に設定する
     */
    public void setFormName(String formName) {
        this.formName = formName;
    }

}
