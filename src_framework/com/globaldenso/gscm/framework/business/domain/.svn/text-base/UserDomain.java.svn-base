package com.globaldenso.gscm.framework.business.domain;

import java.io.Serializable;

/**
 * UserDomainクラス
 * <p>
 * UserDomainクラスです。
 * </p>
 */
public class UserDomain extends AbstractDomain implements Serializable {

    /**
     * シリアルバージョンID。
     */
    private static final long serialVersionUID = 1L;

    /**
     * DSC-ID。
     */
    private String dscId;

    /**
     * 責任会社。
     */
    private String ownerComp;

    /**
     * メールアドレス。
     */
    private String mailAdd1;

    /**
     * ユーザ名称。
     */
    private String usrNm;

    /**
     * 計画製造部工場区分。
     */
    private String prodMfgDivCd;

    /**
     * 部名。
     */
    private String deptName;

    /**
     * 室名。
     */
    private String roomName;

    /**
     * 課名。
     */
    private String sectName;

    /**
     * 役割。
     */
    private String role;


    /**
     * <p>
     * デフォルトコンストラクタ。
     * </p>
     */
    public UserDomain() {
        super();
    }

    /**
     * dscId のゲッターメソッドです。
     *
     * @return dscId
     */
    public String getDscId() {
        return dscId;
    }

    /**
     * dscId のセッターメソッドです。
     *
     * @param dscId dscId に設定する
     */
    public void setDscId(String dscId) {
        this.dscId = dscId;
    }

    /**
     * ownerComp のゲッターメソッドです。
     *
     * @return ownerComp
     */
    public String getOwnerComp() {
        return ownerComp;
    }

    /**
     * ownerComp のセッターメソッドです。
     *
     * @param ownerComp ownerComp に設定する
     */
    public void setOwnerComp(String ownerComp) {
        this.ownerComp = ownerComp;
    }

    /**
     * mailAdd1 のゲッターメソッドです。
     *
     * @return mailAdd1
     */
    public String getMailAdd1() {
        return mailAdd1;
    }

    /**
     * mailAdd1 のセッターメソッドです。
     *
     * @param mailAdd1 mailAdd1 に設定する
     */
    public void setMailAdd1(String mailAdd1) {
        this.mailAdd1 = mailAdd1;
    }

    /**
     * usrNm のゲッターメソッドです。
     *
     * @return usrNm
     */
    public String getUsrNm() {
        return usrNm;
    }

    /**
     * usrNm のセッターメソッドです。
     *
     * @param usrNm usrNm に設定する
     */
    public void setUsrNm(String usrNm) {
        this.usrNm = usrNm;
    }

    /**
     * prodMfgDivCd のゲッターメソッドです。
     *
     * @return prodMfgDivCd
     */
    public String getProdMfgDivCd() {
        return prodMfgDivCd;
    }

    /**
     * prodMfgDivCd のセッターメソッドです。
     *
     * @param prodMfgDivCd prodMfgDivCd に設定する
     */
    public void setProdMfgDivCd(String prodMfgDivCd) {
        this.prodMfgDivCd = prodMfgDivCd;
    }

    /**
     * deptName のゲッターメソッドです。
     *
     * @return deptName
     */
    public String getDeptName() {
        return deptName;
    }

    /**
     * deptName のセッターメソッドです。
     *
     * @param deptName deptName に設定する
     */
    public void setDeptName(String deptName) {
        this.deptName = deptName;
    }

    /**
     * roomName のゲッターメソッドです。
     *
     * @return roomName
     */
    public String getRoomName() {
        return roomName;
    }

    /**
     * roomName のセッターメソッドです。
     *
     * @param roomName roomName に設定する
     */
    public void setRoomName(String roomName) {
        this.roomName = roomName;
    }

    /**
     * sectName のゲッターメソッドです。
     *
     * @return sectName
     */
    public String getSectName() {
        return sectName;
    }

    /**
     * sectName のセッターメソッドです。
     *
     * @param sectName sectName に設定する
     */
    public void setSectName(String sectName) {
        this.sectName = sectName;
    }

    /**
     * role のゲッターメソッドです。
     *
     * @return role
     */
    public String getRole() {
        return role;
    }

    /**
     * role のセッターメソッドです。
     *
     * @param role role に設定する
     */
    public void setRole(String role) {
        this.role = role;
    }


}
