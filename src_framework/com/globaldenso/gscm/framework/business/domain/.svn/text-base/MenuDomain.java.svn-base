package com.globaldenso.gscm.framework.business.domain;

import java.io.Serializable;

/**
 * MenuDomainクラス
 * <p>
 * MenuDomainクラスです。
 * </p>
 */
public class MenuDomain extends AbstractDomain implements Serializable {

    /**
     * シリアルバージョンID。
     */
    private static final long serialVersionUID = 1L;

    /**
     * アクションパス。
     */
    private String actionPath;

    /**
     * 親画面ID。
     */
    private String parScreenId;

    /**
     * 親情報。
     */
    private String parentInfo;

    /**
     * 親総数。
     */
    private String parentTotal;

    /**
     * 表示順。
     */
    private String dispOdr;

    /**
     * メニューラベル。
     */
    private String screenNm;

    /**
     * 画面ID。
     */
    private String screenId;

    /**
     * 子メニュー一覧。
     */
    private String childrenMenuList;

    /**
     * 役割。
     */
    private String role;


    /**
     * <p>
     * デフォルトコンストラクタ。
     * </p>
     */
    public MenuDomain() {
        super();
    }

    /**
     * actionPath のゲッターメソッドです。
     *
     * @return actionPath
     */
    public String getActionPath() {
        return actionPath;
    }

    /**
