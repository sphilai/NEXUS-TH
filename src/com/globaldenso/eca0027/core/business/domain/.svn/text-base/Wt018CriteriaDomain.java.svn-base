package com.globaldenso.eca0027.core.business.domain;

import com.globaldenso.gscm.framework.business.domain.AbstractDomain;

/**
 * The PL layer domain class that holds the criteria information of Containe No. Info Sub Screen.
 * <br/>コンテナNo.情報表示子画面一覧部の検索条件を保持するPL層用のドメインクラスです。
 *
 * @author $Author$
 * @version $Revision$
 */
public class Wt018CriteriaDomain extends AbstractDomain {

    /**
     * SERIAL VERSION ID/シリアルバージョンID
     */
    private static final long serialVersionUID = 1L;

    /**
     * SHIPPING ACTUALITY NO/船積実績No.
     */
    private String shippingActNo;

    /**
     * Default constructor/デフォルトコンストラクタ。
     */
    public Wt018CriteriaDomain() {
    }

    /**
     * <p>shippingActNo のゲッターメソッドです。</p>
     *
     * @return the shippingActNo
     */
    public String getShippingActNo() {
        return shippingActNo;
    }

    /**
     * <p>shippingActNo のセッターメソッドです。</p>
     *
     * @param shippingActNo shippingActNo に設定する
     */
    public void setShippingActNo(String shippingActNo) {
        this.shippingActNo = shippingActNo;
    }
    
}
