package com.globaldenso.eca0027.core.business.domain;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlElement;

/**
 * Item No. level domain used by Ws6011
 * <br />Ws6011で使用する品番レベルのドメインです。
 *
 * @author $Author: masaki_awata@dnitsol.com $
 * @version $Revision: 13073 $
 */
@XmlAccessorType(XmlAccessType.NONE)
public class Ws6011ItemDomain {

    /**
     * Customer code
     */
    private String cusno;

    /**
     * Customer part No.
     */
    @XmlElement(name = "customerItemNo")
    private String cprtn;

    /**
     * Reference date
     * 基準日
     */
    private String refDt;

    /**
     * Item description
     */
    @XmlElement(name = "itemDescription")
    private String cpdsc;

    /**
     * Library name
     */
    private String lib;

    /**
     * Constructor.
     */
    public Ws6011ItemDomain() {
    }

    /**
     * Getter method for cusno.
     *
     * @return the cusno
     */
    public String getCusno() {
        return cusno;
    }

    /**
     * Setter method for cusno.
     *
     * @param cusno Set for cusno
     */
    public void setCusno(String cusno) {
        this.cusno = cusno;
    }

    /**
     * Getter method for cprtn.
     *
     * @return the cprtn
     */
    public String getCprtn() {
        return cprtn;
    }

    /**
     * Setter method for cprtn.
     *
     * @param cprtn Set for cprtn
     */
    public void setCprtn(String cprtn) {
        this.cprtn = cprtn;
    }

    /**
     * Getter method for refDt.
     *
     * @return the refDt
     */
    public String getRefDt() {
        return refDt;
    }

    /**
     * Setter method for refDt.
     *
     * @param refDt Set for refDt
     */
    public void setRefDt(String refDt) {
        this.refDt = refDt;
    }

    /**
     * Getter method for cpdsc.
     *
     * @return the cpdsc
     */
    public String getCpdsc() {
        return cpdsc;
    }

    /**
     * Setter method for cpdsc.
     *
     * @param cpdsc Set for cpdsc
     */
    public void setCpdsc(String cpdsc) {
        this.cpdsc = cpdsc;
    }

    /**
     * Getter method for lib.
     *
     * @return the lib
     */
    public String getLib() {
        return lib;
    }

    /**
     * Setter method for lib.
     *
     * @param lib Set for lib
     */
    public void setLib(String lib) {
        this.lib = lib;
    }
}
