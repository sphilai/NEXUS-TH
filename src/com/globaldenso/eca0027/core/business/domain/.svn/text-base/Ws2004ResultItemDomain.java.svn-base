package com.globaldenso.eca0027.core.business.domain;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlElement;

/**
 * The domain that contains the Name information(CML for) .
 * <br />品名情報(CML用)を格納するドメインです。
 *
 * @author $Author: 810833843 $
 * @version $Revision: 9972 $
 */
@XmlAccessorType(XmlAccessType.FIELD)
public class Ws2004ResultItemDomain {

    /**
     * Serial version
     * <br />シリアルバージョン
     */
    private static final long serialVersionUID = 1L;

    /**
     * Item Number
     */
    @XmlElement(name = "itemNo")
    private String itnbr;
    
    /**
     * Packing Code
     */
    @XmlElement(name = "plantCD")
    private String packc;
    
    /**
     * Unit of Measure
     */
    @XmlElement(name = "unitMeasure")
    private String unmsr;
        
    /**
     * On Hand Total Qty
     */
    @XmlElement(name = "onHandTotalQty")
    private String mohtq;
    
    /**
     * Currency Code(3DIGIT)
     */
    @XmlElement(name = "currencyCd")
    private String curr3;
    
    /**
     * Constructor
     * <br />コンストラクタ
     *
     */
    public Ws2004ResultItemDomain() {
        super();
    }

    /**
     * Getter method for itnbr.
     *
     * @return the itnbr
     */
    public String getItnbr() {
        return itnbr;
    }

    /**
     * Setter method for itnbr.
     *
     * @param itnbr Set for itnbr
     */
    public void setItnbr(String itnbr) {
        this.itnbr = itnbr;
    }

    /**
     * Getter method for packc.
     *
     * @return the packc
     */
    public String getPackc() {
        return packc;
    }

    /**
     * Setter method for packc.
     *
     * @param packc Set for packc
     */
    public void setPackc(String packc) {
        this.packc = packc;
    }

    /**
     * Getter method for unmsr.
     *
     * @return the unmsr
     */
    public String getUnmsr() {
        return unmsr;
    }

    /**
     * Setter method for unmsr.
     *
     * @param unmsr Set for unmsr
     */
    public void setUnmsr(String unmsr) {
        this.unmsr = unmsr;
    }

    /**
     * Getter method for mohtq.
     *
     * @return the mohtq
     */
    public String getMohtq() {
        return mohtq;
    }

    /**
     * Setter method for mohtq.
     *
     * @param mohtq Set for mohtq
     */
    public void setMohtq(String mohtq) {
        this.mohtq = mohtq;
    }

    /**
     * Getter method for curr3.
     *
     * @return the curr3
     */
    public String getCurr3() {
        return curr3;
    }

    /**
     * Setter method for curr3.
     *
     * @param curr3 Set for curr3
     */
    public void setCurr3(String curr3) {
        this.curr3 = curr3;
    }
}
