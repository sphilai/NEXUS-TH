package com.globaldenso.eca0027.core.business.domain.criteria;

import com.globaldenso.gscm.framework.business.domain.AbstractDomain;

/**
 * 
 * Store the search criteria parameters for the service.
 * サービスの検索条件パラメータを格納します。 
 * 
 * @author $Author: masaki_awata@dnitsol.com $
 * @version $Revision: 9946 $
 */
public class WsB009CriteriaDomain extends AbstractDomain {

    /**
     * Default Serial Version ID.
     * デフォルト・シリアル・バージョンID
     */
    private static final long serialVersionUID = 1L;
    
    /**
     * Library Name.
     * ライブラリ名
     */
    private String lib;
    
    /**
     * SHIPPING 24H CORRESPONDENCE FUNCTION.
     * 24H出荷対応機能
     */
    private boolean isShipping24hFunc;
    
    /**
     * Receiving Company Code
     */
    private String receivingCompanyCode;
    
    /**
     * CIGMA_ADS
     */
    private String ads;
    
    /**
     * Job name.
     * Job ジョブ名
     */
    private String jobDescription;
    
    /**
     * Source job ID
     * 呼び出しジョブ元ID
     */
    private String callerJobId;
    
    /**
     * The defulat constructor.
     * デフォルトコンストラクタです。
     */
    public WsB009CriteriaDomain() {
        super();
    }

    /**
     * Getter method for lib.
     *
     * @return the lib
     */
    public String getLib() {
        return lib;
    }

    /**
     * Setter method for lib.
     *
     * @param lib Set for lib
     */
    public void setLib(String lib) {
        this.lib = lib;
    }

    /**
     * Getter method for isShipping24hFunc.
     *
     * @return the isShipping24hFunc
     */
    public boolean isShipping24hFunc() {
        return isShipping24hFunc;
    }

    /**
     * Setter method for isShipping24hFunc.
     *
     * @param isShipping24hFunc Set for isShipping24hFunc
     */
    public void setShipping24hFunc(boolean isShipping24hFunc) {
        this.isShipping24hFunc = isShipping24hFunc;
    }

    /**
     * Getter method for jobDescription.
     *
     * @return the jobDescription
     */
    public String getJobDescription() {
        return jobDescription;
    }

    /**
     * Setter method for jobDescription.
     *
     * @param jobDescription Set for jobDescription
     */
    public void setJobDescription(String jobDescription) {
        this.jobDescription = jobDescription;
    }

    /**
     * Getter method for receivingCompanyCode.
     *
     * @return the receivingCompanyCode
     */
    public String getReceivingCompanyCode() {
        return receivingCompanyCode;
    }

    /**
     * Setter method for receivingCompanyCode.
     *
     * @param receivingCompanyCode Set for receivingCompanyCode
     */
    public void setReceivingCompanyCode(String receivingCompanyCode) {
        this.receivingCompanyCode = receivingCompanyCode;
    }

    /**
     * Getter method for ads.
     *
     * @return the ads
     */
    public String getAds() {
        return ads;
    }

    /**
     * Setter method for ads.
     *
     * @param ads Set for ads
     */
    public void setAds(String ads) {
        this.ads = ads;
    }
    
    /**
     * Getter method for callerJobId.
     *
     * @return the callerJobId
     */
    public String getCallerJobId() {
        return callerJobId;
    }

    /**
     * Setter method for callerJobId.
     *
     * @param callerJobId Set for callerJobId
     */
    public void setCallerJobId(String callerJobId) {
        this.callerJobId = callerJobId;
    }
}
