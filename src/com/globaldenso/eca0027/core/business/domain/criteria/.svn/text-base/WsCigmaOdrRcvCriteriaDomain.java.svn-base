package com.globaldenso.eca0027.core.business.domain.criteria;

import com.globaldenso.gscm.framework.business.domain.AbstractDomain;

/**
 * 
 * The domain class that holds the search criteria for the Receive Order.
 * 
 * @author $Author: masaki_awata@dnitsol.com $
 * @version $Revision: 9946 $
 */
public class WsCigmaOdrRcvCriteriaDomain extends AbstractDomain {

    /**
     * Serial version ID
     */
    private static final long serialVersionUID = 1L;

    /**
     * Receiving Company Code
     */
    private String receivingCompanyCode;
    
    /**
     * CIGMA ADS
     */
    private String ads;
    
    /**
     * Source job ID
     * 呼び出しジョブ元ID
     */
    private String callerJobId;
    
    /**
     * default constructor.
     */
    public WsCigmaOdrRcvCriteriaDomain() {
        super();
    }

    /**
     * Getter method for receivingCompanyCode.
     *
     * @return the receivingCompanyCode
     */
    public String getReceivingCompanyCode() {
        return receivingCompanyCode;
    }

    /**
     * Setter method for receivingCompanyCode.
     *
     * @param receivingCompanyCode Set for receivingCompanyCode
     */
    public void setReceivingCompanyCode(String receivingCompanyCode) {
        this.receivingCompanyCode = receivingCompanyCode;
    }

    /**
     * Getter method for ads.
     *
     * @return the ads
     */
    public String getAds() {
        return ads;
    }

    /**
     * Setter method for ads.
     *
     * @param ads Set for ads
     */
    public void setAds(String ads) {
        this.ads = ads;
    }
    
    /**
     * Getter method for callerJobId.
     *
     * @return the callerJobId
     */
    public String getCallerJobId() {
        return callerJobId;
    }

    /**
     * Setter method for callerJobId.
     *
     * @param callerJobId Set for callerJobId
     */
    public void setCallerJobId(String callerJobId) {
        this.callerJobId = callerJobId;
    }
}
