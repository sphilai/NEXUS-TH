package com.globaldenso.eca0027.core.presentation;


import com.globaldenso.eca0027.core.business.domain.W8002CriteriaDomain;
import com.globaldenso.gscm.framework.presentation.AbstractActionForm;

/**
 * Action form of Progress Control detail screen.
 * <br />進度管理詳細画面のアクション・フォームです。
 *
 * @author $Author: 810833613 $
 * @version $Revision: 7765 $
 */
public class W8002ActionForm extends AbstractActionForm {

    /**
     * Serial version ID
     * <br />シリアルバージョンID
     */
    private static final long serialVersionUID = 1L;

    /**
     * Search conditions domain
     * <br />検索条件ドメイン
     */
    private W8002CriteriaDomain w8002CriteriaDomain;

    /**
     * Takeover VALUE MAIN MARK
     * <br />引継値 メインマーク
     */
    private String cmlNo;
    /**
     * Takeover VALUE CML TYPE
     * <br />引継値 ケースマーク区分
     */
    private String cmlTyp;
    
    /**
     * Type in the functional overview of the constructor.
     *
     */
    public W8002ActionForm() {
        w8002CriteriaDomain = new W8002CriteriaDomain();
    }

    /**
     * Getter method for w8002CriteriaDomain.
     *
     * @return the w8002CriteriaDomain
     */
    public W8002CriteriaDomain getW8002CriteriaDomain() {
        return w8002CriteriaDomain;
    }

    /**
     * Setter method for w8002CriteriaDomain.
     *
     * @param criteriaDomain Set for w8002CriteriaDomain
     */
    public void setW8002CriteriaDomain(W8002CriteriaDomain criteriaDomain) {
        w8002CriteriaDomain = criteriaDomain;
    }

    /**
     * Getter method for cmlNo.
     *
     * @return the cmlNo
     */
    public String getCmlNo() {
        return cmlNo;
    }

    /**
     * Setter method for cmlNo.
     *
     * @param cmlNo Set for cmlNo
     */
    public void setCmlNo(String cmlNo) {
        this.cmlNo = cmlNo;
    }

    /**
     * Getter method for cmlTyp.
     *
     * @return the cmlTyp
     */
    public String getCmlTyp() {
        return cmlTyp;
    }

    /**
     * Setter method for cmlTyp.
     *
     * @param cmlTyp Set for cmlTyp
     */
    public void setCmlTyp(String cmlTyp) {
        this.cmlTyp = cmlTyp;
    }

}
