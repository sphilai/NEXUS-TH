package com.globaldenso.eca0027.core.common.business.domain;

import com.globaldenso.gscm.framework.business.domain.AbstractDomain;

/**
 * The domain class that stores use authority of user's function
 * <br />ユーザの機能利用権限を格納するドメインです。
 *
 * @author $Author: 10088DS01586 $
 * @version $Revision: 5759 $
 */
public class UserAuthDomain extends AbstractDomain {
    /**
     * serial version <br />
     * シリアルバージョン。
     */
    private static final long serialVersionUID = 1L;

    /**
     * DSC-ID
     */
    private String dscId;
    
    /**
     * role <br />
     * 役割
     */
    private String role;
    
    /**
     * company that manages the authority <br />
     * 権限付与会社
     */
    private String authMgtComp;
    
    /**
     * PLANT CODE (process) <br />
     * 製造部工場区分（工程）
     */
    private String procMfgDivCd;
    
    /**
     * Authorization function ID <br />
     * 認可機能ID
     */
    private String permitFuncId;
    
    /**
     * authority (B:access authority/R:update authority) <br >
     * 権限 (B:参照権限／R:更新権限)
     */
    private String authority;

    /**
     * constructor.
     * <br />デフォルトコンストラクタです。
     */
    public UserAuthDomain() {
    }

    /**
     * Getter method for dscId.
     *
     * @return the dscId
     */
    public String getDscId() {
        return dscId;
    }

    /**
     * Setter method for dscId.
     *
     * @param dscId Set for dscId
     */
    public void setDscId(String dscId) {
        this.dscId = dscId;
    }

    /**
     * Getter method for role.
     *
     * @return the role
     */
    public String getRole() {
        return role;
    }

    /**
     * Setter method for role.
     *
     * @param role Set for role
     */
    public void setRole(String role) {
        this.role = role;
    }

    /**
     * Getter method for authMgtComp.
     *
     * @return the authMgtComp
     */
    public String getAuthMgtComp() {
        return authMgtComp;
    }

    /**
     * Setter method for authMgtComp.
     *
     * @param authMgtComp Set for authMgtComp
     */
    public void setAuthMgtComp(String authMgtComp) {
        this.authMgtComp = authMgtComp;
    }

    /**
     * Getter method for procMfgDivCd.
     *
     * @return the procMfgDivCd
     */
    public String getProcMfgDivCd() {
        return procMfgDivCd;
    }

    /**
     * Setter method for procMfgDivCd.
     *
     * @param procMfgDivCd Set for procMfgDivCd
     */
    public void setProcMfgDivCd(String procMfgDivCd) {
        this.procMfgDivCd = procMfgDivCd;
    }

    /**
     * Getter method for permitFuncId.
     *
     * @return the permitFuncId
     */
    public String getPermitFuncId() {
        return permitFuncId;
    }

    /**
     * Setter method for permitFuncId.
     *
     * @param permitFuncId Set for permitFuncId
     */
    public void setPermitFuncId(String permitFuncId) {
        this.permitFuncId = permitFuncId;
    }

    /**
     * Getter method for authority.
     *
     * @return the authority
     */
    public String getAuthority() {
        return authority;
    }

    /**
     * Setter method for authority.
     *
     * @param authority Set for authority
     */
    public void setAuthority(String authority) {
        this.authority = authority;
    }

}
