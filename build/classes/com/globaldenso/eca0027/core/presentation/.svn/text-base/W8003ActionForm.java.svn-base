package com.globaldenso.eca0027.core.presentation;

import java.util.List;

import com.globaldenso.eca0027.core.business.domain.W8003CriteriaDomain;
import com.globaldenso.eca0027.core.business.domain.W8003ListDomain;
import com.globaldenso.gscm.framework.presentation.AbstractActionForm;

/**
 * Action form of Progress Control case mark list screen.
 * <br />進度管理ケースマーク一覧画面のアクション・フォームです。
 *
 * @author $Author: 810833613 $
 * @version $Revision: 7765 $
 */
public class W8003ActionForm extends AbstractActionForm {

    /**
     * Serial version ID
     * <br />シリアルバージョンID
     */
    private static final long serialVersionUID = 1L;

    /**
     * Search conditions domain
     * <br />検索条件ドメイン
     */
    private W8003CriteriaDomain w8003CriteriaDomain;
    
    /**
     * List of search results domain
     * <br />検索結果ドメインのリスト
     */
    private List<? extends W8003ListDomain> w8003ListDomainList;

    /**
     * Search results display area flag
     * <br />検索結果エリア表示フラグ
     */
    private boolean displayList;

    /**
     * Takeover VALUE MAIN MARK
     * <br />引継値 メインマーク
     */
    private String cmlNo;
    
    /**
     * Takeover VALUE CML TYPE
     * <br />引継値 ケースマーク区分
     */
    private String cmlTyp;
    
   
    /**
     * Default constructor.
     * <br />デフォルトコンストラクタです。
     */
    public W8003ActionForm() {
        w8003CriteriaDomain = new W8003CriteriaDomain();
    }


    /**
     * Getter method for w8003CriteriaDomain.
     *
     * @return the w8003CriteriaDomain
     */
    public W8003CriteriaDomain getW8003CriteriaDomain() {
        return w8003CriteriaDomain;
    }


    /**
     * Setter method for w8003CriteriaDomain.
     *
     * @param criteriaDomain Set for w8003CriteriaDomain
     */
    public void setW8003CriteriaDomain(W8003CriteriaDomain criteriaDomain) {
        w8003CriteriaDomain = criteriaDomain;
    }


    /**
     * Getter method for w8003ListDomainList.
     *
     * @return the w8003ListDomainList
     */
    public List<? extends W8003ListDomain> getW8003ListDomainList() {
        return w8003ListDomainList;
    }


    /**
     * Setter method for w8003ListDomainList.
     *
     * @param listDomainList Set for w8003ListDomainList
     */
    public void setW8003ListDomainList(
        List<? extends W8003ListDomain> listDomainList) {
        w8003ListDomainList = listDomainList;
    }


    /**
     * Getter method for displayList.
     *
     * @return the displayList
     */
    public boolean isDisplayList() {
        return displayList;
    }


    /**
     * Setter method for displayList.
     *
     * @param displayList Set for displayList
     */
    public void setDisplayList(boolean displayList) {
        this.displayList = displayList;
    }


    /**
     * Getter method for cmlNo.
     *
     * @return the cmlNo
     */
    public String getCmlNo() {
        return cmlNo;
    }


    /**
     * Setter method for cmlNo.
     *
     * @param cmlNo Set for cmlNo
     */
    public void setCmlNo(String cmlNo) {
        this.cmlNo = cmlNo;
    }


    /**
     * Getter method for cmlTyp.
     *
     * @return the cmlTyp
     */
    public String getCmlTyp() {
        return cmlTyp;
    }


    /**
     * Setter method for cmlTyp.
     *
     * @param cmlTyp Set for cmlTyp
     */
    public void setCmlTyp(String cmlTyp) {
        this.cmlTyp = cmlTyp;
    }
    
    
}
