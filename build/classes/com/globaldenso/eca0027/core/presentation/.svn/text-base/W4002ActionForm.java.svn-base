package com.globaldenso.eca0027.core.presentation;

import com.globaldenso.eca0027.core.business.domain.W4002CriteriaDomain;
import com.globaldenso.gscm.framework.presentation.AbstractActionForm;

/**
 * The action form class of Staging Instruction Header Screen
 * <br />荷揃え指示Header画面のアクションフォームクラスです。
 *
 * @author $Author: masaki_awata@dnitsol.com $
 * @version $Revision: 5446 $
 */
public class W4002ActionForm extends AbstractActionForm {

    /**
     * Serial version.
     */
    private static final long serialVersionUID = 1L;

    /**
     * Header infomation for creating Staging Instruction.
     */
    private W4002CriteriaDomain w4002CriteriaDomain;

    /**
     * constructor.
     */
    public W4002ActionForm() {
        super();
    }

    /**
     * Getter method for criteriaDomain.
     *
     * @return the criteriaDomain
     */
    public W4002CriteriaDomain getW4002CriteriaDomain() {
        if (w4002CriteriaDomain == null) {
            w4002CriteriaDomain = new W4002CriteriaDomain();
        }
        return w4002CriteriaDomain;
    }

    /**
     * Setter method for w4002CriteriaDomain.
     *
     * @param criteriaDomain Set for w4002CriteriaDomain
     */
    public void setW4002CriteriaDomain(W4002CriteriaDomain criteriaDomain) {
        w4002CriteriaDomain = criteriaDomain;
    }
}
