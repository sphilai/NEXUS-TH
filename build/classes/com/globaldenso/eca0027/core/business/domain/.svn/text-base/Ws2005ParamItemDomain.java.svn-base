package com.globaldenso.eca0027.core.business.domain;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlElement;

/**
 * The domain class that stores information repetition of the request parameter of Ws2005.
 * <br />Ws2005のリクエストパラメータの繰り返し情報を格納するドメインクラスです。
 *
 * @author $Author: 10088DS01586 $
 * @version $Revision: 5759 $
 */
@XmlAccessorType(XmlAccessType.FIELD)
public class Ws2005ParamItemDomain {

    /**
     * Serial version
     * <br />シリアルバージョン
     */
    private static final long serialVersionUID = 1L;

    /**
     * Customers No
     * <br />得意先No
     */
    @XmlElement(name = "customerNo")
    private String customerNo;
    
    /**
     * Ship to
     * <br />送荷先
     */
    @XmlElement(name = "shipTo")
    private String shipTo;
    
    /**
     * Item No ..
     * <br />品目番号
     */
    @XmlElement(name = "pn")
    private String pN;
    
    /**
     * Transport Code
     * <br />輸送手段コード
     */
    @XmlElement(name = "transWayCd")
    private String transWayCd;
    
    /**
     * Vessel product deadline
     * <br />船積期限
     */
    @XmlElement(name = "etdDueDt")
    private String etdDueDt;
    
    /**
     * Customers PO NO
     * <br />得意先PO NO
     */
    @XmlElement(name = "customerPoNo")
    private String customerPoNo;
    
    /**
     * Customers Item No ..
     * <br />得意先品目番号
     */
    @XmlElement(name = "customerPn")
    private String customerPn;
    
    /**
     * No order control
     * <br />オーダーコントロールNo
     */
    @XmlElement(name = "odrCtrlNo")
    private String odrCtrlNo;
    
    /**
     * Customers Item No .. original
     * <br />得意先品目番号オリジナル
     */
    @XmlElement(name = "customerPnOrg")
    private String customerPnOrg;
    
    /**
     * Shipping instructions provision already Contents1_Qty
     * <br />出荷指示引当済数量
     */
    @XmlElement(name = "shipInstrAllocQty")
    private String shipInstrAllocQty;

    /**
     * Constructor
     * <br />コンストラクタ
     */
    public Ws2005ParamItemDomain() {
    }

    /**
     * Getter method for customerNo.
     *
     * @return the customerNo
     */
    public String getCustomerNo() {
        return customerNo;
    }

    /**
     * Setter method for customerNo.
     *
     * @param customerNo Set for customerNo
     */
    public void setCustomerNo(String customerNo) {
        this.customerNo = customerNo;
    }

    /**
     * Getter method for shipTo.
     *
     * @return the shipTo
     */
    public String getShipTo() {
        return shipTo;
    }

    /**
     * Setter method for shipTo.
     *
     * @param shipTo Set for shipTo
     */
    public void setShipTo(String shipTo) {
