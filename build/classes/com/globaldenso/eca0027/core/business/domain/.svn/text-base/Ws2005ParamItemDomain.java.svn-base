     *
     * @param customerPnOrg Set for customerPnOrg
     */
    public void setCustomerPnOrg(String customerPnOrg) {
        this.customerPnOrg = customerPnOrg;
    }

    /**
     * Getter method for shipInstrAllocQty.
     *
     * @return the shipInstrAllocQty
     */
    public String getShipInstrAllocQty() {
        return shipInstrAllocQty;
    }

    /**
     * Setter method for shipInstrAllocQty.
     *
     * @param shipInstrAllocQty Set for shipInstrAllocQty
     */
    public void setShipInstrAllocQty(String shipInstrAllocQty) {
