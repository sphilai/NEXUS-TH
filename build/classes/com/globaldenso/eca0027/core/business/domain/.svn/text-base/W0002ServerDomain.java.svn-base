package com.globaldenso.eca0027.core.business.domain;

import com.globaldenso.gscm.framework.business.domain.AbstractDomain;

/**
 * The domain class that holds the detail information of Server
 * <br />サーバ情報を保持するBL層のドメインクラスです。
 *
 * @author $Author: 10088DS01586 $
 * @version $Revision: 5759 $
 */
public class W0002ServerDomain extends AbstractDomain {

    /**
     * Serial Version <br />
     * シリアルバージョン
     */
    private static final long serialVersionUID = 1L;

    /**
     * Server Name
     * <br />名称
     */
    private String serverNm;

    /**
     * <br />URL
     */
    private String url;
    
    /**
     * Constructor
     * <br />デフォルトコンストラクタです。
     */
    public W0002ServerDomain() {
    }

    /**
     * Getter method for serverNm.
     *
     * @return the serverNm
     */
    public String getServerNm() {
        return serverNm;
    }

    /**
     * Setter method for serverNm.
     *
     * @param serverNm Set for serverNm
     */
    public void setServerNm(String serverNm) {
        this.serverNm = serverNm;
    }

    /**
     * Getter method for url.
     *
     * @return the url
     */
    public String getUrl() {
        return url;
    }

    /**
     * Setter method for url.
     *
     * @param url Set for url
     */
    public void setUrl(String url) {
        this.url = url;
    }


}
