    public W9002CriteriaDomain getCriteriaDomain() {
        return criteriaDomain;
    }

    /**
     * Setter method for criteriaDomain.
     *
     * @param criteriaDomain Set for criteriaDomain
     */
    public void setCriteriaDomain(W9002CriteriaDomain criteriaDomain) {
        this.criteriaDomain = criteriaDomain;
    }
    
    /**
     * Getter method for listDomainList.
     *
     * @return the listDomainList
     */
    public List<? extends W9002ListDomain> getListDomainList() {
        return listDomainList;
    }

    /**
