package com.globaldenso.eca0027.core.business.domain;

import java.util.List;

import com.globaldenso.gscm.framework.business.domain.AbstractDomain;

/**
 * The domain class for PL layer that holds the menu information
 * <br />メニュー情報を保持するPL層のドメインクラスです。
 *
 * @author $Author: 10088DS01586 $
 * @version $Revision: 5759 $
 */
public class W0002MenuListDomain extends AbstractDomain {

    /**
     * Serial Version <br />
     * シリアルバージョン
     */
    private static final long serialVersionUID = 1L;

    /**
     * ID
     */
    private String id;
    
    /**
     * Screen ID
     * <br />画面ID
     */
    private String screenId;
    
    /**
     * Screen Name
     * <br />画面名
     */
    private String screenNm;
    
    /**
     * Parent Screen ID
     * <br />親画面ID
     */
    private String parentScreenId;
    
    /**
     * Action Path
     * <br />アクションパス
     */
    private String actionPath;
    
    /**
     * Permission Flag
     * <br />利用可否フラグ
     */
    private boolean permitted;
    
    /**
     * Sub Menu
     * <br />サブメニュー
     */
    private List<? extends W0002MenuListDomain> subMenuList;
    
    /**
     * Constructor
     * <br />デフォルトコンストラクタです。
     */
    public W0002MenuListDomain() {
    }

    /**
     * Getter method for id.
     *
     * @return the id
     */
    public String getId() {
        return id;
    }

    /**
     * Setter method for id.
     *
     * @param id Set for id
     */
    public void setId(String id) {
        this.id = id;
    }

    /**
     * Getter method for screenId.
     *
     * @return the screenId
     */
    public String getScreenId() {
        return screenId;
    }

    /**
     * Setter method for screenId.
     *
     * @param screenId Set for screenId
     */
    public void setScreenId(String screenId) {
        this.screenId = screenId;
    }

    /**
     * Getter method for screenNm.
     *
     * @return the screenNm
     */
    public String getScreenNm() {
        return screenNm;
    }

    /**
     * Setter method for screenNm.
     *
     * @param screenNm Set for screenNm
     */
    public void setScreenNm(String screenNm) {
        this.screenNm = screenNm;
    }

    /**
     * Getter method for parentScreenId.
     *
     * @return the parentScreenId
     */
    public String getParentScreenId() {
        return parentScreenId;
    }

    /**
     * Setter method for parentScreenId.
     *
     * @param parentScreenId Set for parentScreenId
     */
    public void setParentScreenId(String parentScreenId) {
        this.parentScreenId = parentScreenId;
    }

    /**
     * Getter method for actionPath.
     *
     * @return the actionPath
     */
    public String getActionPath() {
        return actionPath;
    }

    /**
     * Setter method for actionPath.
     *
     * @param actionPath Set for actionPath
     */
    public void setActionPath(String actionPath) {
        this.actionPath = actionPath;
    }

    /**
     * Getter method for permitted.
     *
     * @return the permitted
     */
    public boolean isPermitted() {
        return permitted;
    }

    /**
     * Setter method for permitted.
     *
     * @param permitted Set for permitted
     */
    public void setPermitted(boolean permitted) {
        this.permitted = permitted;
    }

    /**
     * Getter method for subMenuList.
     *
     * @return the subMenuList
     */
    public List<? extends W0002MenuListDomain> getSubMenuList() {
        return subMenuList;
    }

    /**
     * Setter method for subMenuList.
     *
     * @param subMenuList Set for subMenuList
     */
    public void setSubMenuList(List<? extends W0002MenuListDomain> subMenuList) {
        this.subMenuList = subMenuList;
    }
}
