package com.globaldenso.gscm.framework.util;

import java.math.BigDecimal;
import java.util.Date;
import java.util.List;
import java.util.Map;

import com.globaldenso.gscm.framework.presentation.AbstractActionForm;

/**
 * CopyPropertiesメソッドテスト用ActionFormクラス
 *
 * @author $Author: 10088DS01586 $
 * @version $Revision: 5759 $
 */
public class CopyPropertiesTest2ActionForm extends AbstractActionForm {
    /** シリアルバージョンID */
    private static final long serialVersionUID = 1L;
    /** 日付文字列1 */
    private Date date1 = null;
    /** 日付文字列2 */
    private Date date2 = null;
    /** 項目 */
    private String item = null;
    /** Listデータ */
    private List<String> list = null;
    /** Mapデータ */
    private Map<String, String> map = null;
    /** 日付 */
    private Date testDate = null;
    /** BigDecimalデータ */
    private BigDecimal decimal = null;

    /**
     * デフォルトコンストラクタ
     */
    public CopyPropertiesTest2ActionForm() {
    }

    /**
     * date1の取得
     * 
     * @return date1
     */
    public Date getDate1() {
        return date1;
    }

    /**
     * date1の設定
     * 
     * @param date1 date1
     */
    public void setDate1(Date date1) {
        this.date1 = date1;
    }

    /**
     * date2の取得
     * 
     * @return date2
     */
    public Date getDate2() {
        return date2;
    }

    /**
     * date2の設定
     * 
     * @param date2 date2
     */
    public void setDate2(Date date2) {
        this.date2 = date2;
    }

    /**
     * itemの取得
     * 
     * @return item
     */
    public String getItem() {
        return item;
    }

    /**
     * itemの設定
     * 
     * @param item item
